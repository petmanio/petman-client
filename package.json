{
  "name": "@petman/client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:out": "ng serve --aot --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:ssr:prod": "npm run build:client-and-server-bundles:prod && npm run webpack:server:prod",
    "serve:ssr": "node dist/server.js",
    "build:prod": "ng build --prod",
    "build:server": "ng run petman-client:server",
    "build:server:prod": "ng run petman-client:server:production",
    "build:client-and-server-bundles": "npm run build && npm run build:server",
    "build:client-and-server-bundles:prod": "npm run build:prod && npm run build:server:prod",
    "webpack:server": "webpack --mode development --config webpack.server.config.js --progress --colors",
    "webpack:server:prod": "webpack --mode production --config webpack.server.config.js --progress --colors",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -d docs -p src/tsconfig.app.json",
    "compodoc:serve": "./node_modules/.bin/compodoc -d docs -s"
  },
